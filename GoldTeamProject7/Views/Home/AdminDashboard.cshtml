@model GoldTeamProject7.Models.AdminViewModel

@{
    ViewBag.Title = "Admin Dashboard";
}

<div class="row">
    <div class="large-12 columns">
        <table class="user-table" border="0">
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Zipcode</th>
            </tr>
            @foreach (var item in Model.userList)
            {
                <tr>
                    <td>@item.FirstName</td>
                    <td>@item.LastName</td>
                    <td>@item.UserName</td>
                    <td>@item.Zipcode</td>
                </tr>
            }
        </table>
    </div>
</div>